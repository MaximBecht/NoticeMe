<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:media="http://schemas.microsoft.com/winfx/2006/xaml/presentation">

    <!--Background Color Brushes-->
    <SolidColorBrush x:Key="NoticeMe_PrimaryBackgroundColorBrush" Color="#393E46"/>
    <SolidColorBrush x:Key="NoticeMe_SecondaryBackgroundColorBrush" Color="#222831"/>

    <!--Text Color Brushes-->
    <SolidColorBrush x:Key="NoticeMe_SelectedTextColorBrush" Color="#00ADB5"/>
    <SolidColorBrush x:Key="NoticeMe_NormalTextColorBrush" Color="#EEEEEE"/>
    <SolidColorBrush x:Key="NoticeMe_DisabledTextColorBrush" Color="#AAAAAA"/>
    
    <!--Button Color Brushes-->
    <SolidColorBrush x:Key="NoticeMe_DefaultTransparentButtonColorBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="NoticeMe_SelectedButtonIconColorBrush" Color="#00ADB5"/>
    <SolidColorBrush x:Key="NoticeMe_NormalButtonIconColorBrush" Color="#EEEEEE"/>
    <SolidColorBrush x:Key="NoticeMe_DisabledButtonIconColorBrush" Color="#AAAAAA"/>

</ResourceDictionary>
