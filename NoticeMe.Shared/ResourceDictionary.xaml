    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:media="http://schemas.microsoft.com/winfx/2006/xaml/presentation">

    <Style TargetType="StackPanel" x:Key="GlobalStackPanelStyle">
